    "@vue/test-utils": "^1.0.4",
    "autoprefixer": "^9.8.6",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.1.0",
    "chalk": "^2.4.2",
    "codemirror": "^5.62.2",
    "compression-webpack-plugin": "^2.0.0",
    "copy-webpack-plugin": "^4.6.0",
    "css-loader": "^1.0.1",
