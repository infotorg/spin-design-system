    "vue-functional-data-merge": "^3.1.0",
    "vue-lodash": "^2.1.2",
    "vue-meta": "^1.6.0",
    "vue-router": "^3.4.3",
    "vuex": "^3.5.1",
    "webfontloader": "^1.6.28"
  },
